<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>ontobuilder.io</artifactId>
  <name>Ontobuilder IO</name>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <resources>
      <resource>
        <directory>src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <organization>
  	<name>Technion IE&amp;M</name>
  	<url>ie.technion.ac.il</url>
  </organization>
  <scm>
  	<url>https://bitbucket.org/tomers77/ontobuilderdev</url>
  	<developerConnection>tomers77</developerConnection>
  	<connection>ssh</connection>
  </scm>
  <dependencies>
  	<dependency>
  		<groupId>org.jdom</groupId>
  		<artifactId>jdom2</artifactId>
  		<version>2.0.5</version>
  	</dependency>
  	<dependency>
  		<groupId>com.sun.xsom</groupId>
  		<artifactId>xsom</artifactId>
  		<version>20100725</version>
  	</dependency>
  	<dependency>
  		<groupId>net.sourceforge.owlapi</groupId>
  		<artifactId>owlapi-distribution</artifactId>
  		<version>3.4.9</version>
  		<type>bundle</type>
  	</dependency>
  	<dependency>
  		<groupId>net.sourceforge.owlapi</groupId>
  		<artifactId>owlapi-api</artifactId>
  		<version>3.4.9</version>
  		<type>bundle</type>
  	</dependency>
  	<dependency>
  		<groupId>wsdl4j</groupId>
  		<artifactId>wsdl4j</artifactId>
  		<version>1.6.3</version>
  	</dependency>
  	<dependency>
  		<groupId>com.hp.hpl.jena</groupId>
  		<artifactId>jena</artifactId>
  		<version>2.6.4</version>
  	</dependency>
  	<dependency>
  		<groupId>org.jbpt</groupId>
  		<artifactId>jbpt</artifactId>
  		<version>0.2.393</version>
  	</dependency>
  	<dependency>
  		<groupId>org.ow2.easywsdl</groupId>
  		<artifactId>easywsdl-wsdl</artifactId>
  		<version>2.1</version>
  	</dependency>
  	<dependency>
  		<groupId>il.ac.technion.ob</groupId>
  		<artifactId>ontobuilder.core</artifactId>
  		<version>2.2-SNAPSHOT</version>
  	</dependency>
  	<dependency>
  		<groupId>il.ac.technion.ob</groupId>
  		<artifactId>ontobuilder.matching</artifactId>
  		<version>2.2-SNAPSHOT</version>
  	</dependency>
  </dependencies>
  <repositories>
  <repository>
    <id>my-local-repo</id>
    <url>file://${basedir}/../../tomers-repo</url>
  </repository>
  <repository>
      <id>ow2-repo</id> 
      <url>http://repository.ow2.org/nexus</url>
  </repository>
  </repositories>
  <parent>
  	<groupId>il.ac.technion.ob</groupId>
  	<artifactId>OB</artifactId>
  	<version>2.2-SNAPSHOT</version>
  	<relativePath>../../OntobuilderResearchEnvironment/OB</relativePath>
  </parent>
</project>